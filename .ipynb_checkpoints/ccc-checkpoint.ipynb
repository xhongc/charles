   "source": [
    "class Solution:\n",
    "    def translateNum(self, num: int) -> int:\n",
    "        s = str(num)\n",
    "        a,b =1,1\n",
    "        for i in range(2,len(s)):\n",
    "            a,b = (a + b if \"10\" <= s[i - 2:i] <= \"25\" else a), a\n",
    "        return a"
   ]
